{
  "name": "strawserver",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "server.js",
  "scripts": {
    "start": "cross-env concurrently -n Server,Client \"npm run dev:server\" \"npm run client:watch\"",
    "dev:server": "cross-env nodemon -e js,pug server.js --colors",
    "client:watch": "cross-env parcel watch ./client/client.js -d ./public/dist --public-url ./",
    "client:build": "cross-env parcel build ./client/client.js -d ./public/dist --public-url ./",
    "production:server": "cross-env node server.js",
    "production": "npm run client:build && npm run production:server"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "jsx-control-statements"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "dist/*",
      "public/*",
      "node_modules/*"
    ],
    "delay": "500"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "gamedig": "^2.0.20",
    "helmet": "^3.21.2",
    "moment": "^2.29.2",
    "moment-duration-format": "^2.3.2",
    "pug": "^2.0.4",
    "socket.io": "^2.3.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-plugin-jsx-control-statements": "^4.0.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "parcel-bundler": "^1.12.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass": "^1.23.7",
    "socket.io-client": "^2.3.0"
  }
}
